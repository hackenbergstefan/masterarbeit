      #print "frob: i=",i,"gi=",g
      if pows != []:
          if pows.has_key(i):
              xi = pows[i];
          else:
              xi = x**(q**i);
              pows[i] = xi;
      else: xi = x**(q**i);
      #print "\t->x^(q^i)=",xi
      try:
          ret += g*xi;
      except:
          h = Hom(F,E)[0];
          ret += h(g)*xi;
      #print "\t\t->ret=",ret

